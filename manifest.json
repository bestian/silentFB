{
  "name": "SilentFB",
  "description": "Make FB grayscale",
  "icons": { "16": "icon16.png",
  			"48": "icon48.png",
  			"128": "icon128.png" },
  "version": "1.0.1",
  "browser_action": {
    "default_title": "Make Facebook grayscale"
  },
  "content_scripts": [
    {
      "matches": ["*://www.facebook.com/*"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 2
}
