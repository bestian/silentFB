{
  "name": "SilentFB",
  "description": "Make FB grayscale",
  "version": "1.0.1",
  "permissions": [
    "activeTab", "*://www.facebook.com/*", "https://www.facebook.com/*"
  ],
  "browser_action": {
    "default_title": "Make Facebook grayscale"
  },
  "content_scripts": [
    {
      "matches": ["*://www.facebook.com/*"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 2
}
